//PROBLEM 9
#include<iostream>
#include<conio.h>

using namespace std;

int main()
{
	int a, s;
	
	cout<<"How many rows: "; cin>> a;
	cout<<"How many columns: "; cin>> s;
	
	for(int d=1; d<=a; d++)
	{
		cout<<" "<<endl;
		
    for(int f=1; f < s; f++)
		{
			cout<<"*";
		}
		cout << endl;
	}
	
	_getch();
	return 0;
}
