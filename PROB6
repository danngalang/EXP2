//FIBO
#include<iostream>
#include<conio.h>

using namespace std;
int main()

{
	
	int nm1, nm2, NN;
	nm1 = 0;
	nm2 = 1;
	
	cout << "Fibonacci numbers:" << endl;
	
	cout <<nm1 << "," << nm2 << "," ;
	
	for(int n = 3; n<=22; n++)
	{
		
		NN = nm1 + nm2;
		nm1 = nm2;
		nm2 = NN;
		
	
		cout << NN;
		
		if(NN < 10946)
		{
			cout<<",";
		}
	}
	
	_getch();
	return 0;
}
